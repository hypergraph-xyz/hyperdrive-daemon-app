{
  "name": "hyperdrive-daemon-app",
  "productName": "Hyperdrive Daemon",
  "private": true,
  "version": "0.0.3",
  "license": "MIT",
  "repository": "hypergraph-xyz/hyperdrive-daemon-app",
  "scripts": {
    "dist": "electron-builder",
    "pack": "electron-builder --dir",
    "release": "np --no-publish --no-yarn",
    "start": "nodemon --exec electron index.js",
    "test": "prettier-standard && standard"
  },
  "devDependencies": {
    "electron": "^7.1.1",
    "electron-builder": "^22.4.1",
    "nodemon": "^2.0.3",
    "np": "^6.2.1",
    "prettier-standard": "^16.2.1",
    "standard": "^14.3.3"
  },
  "dependencies": {
    "electron-store": "^5.1.1",
    "hyperdrive-daemon": "^1.10.3",
    "hyperdrive-daemon-client": "^1.11.2",
    "hyperdrive-fuse": "^1.2.15",
    "sudo-prompt": "^9.1.1"
  },
  "build": {
    "mac": {
      "extendInfo": {
        "LSUIElement": 1
      }
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
